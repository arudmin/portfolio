!!!5
html
  head
    title Greg Allen
    script(src="ui/vendor/live/live.js")
    link(rel="stylesheet", href="ui/_compressed/common.css")
  body.preanim
    div.frame
      div.header.col3
        div.box.col.me
          img(src="ui/images/me.jpg", width="75")
          h1 Greg Allen
          h2 Developer
        div.box.col.twitter
          p twitter
        div.box.col.github
          p github
      ul.nav.hl
        li.all: a(href="#") All
          span.line
        li.sites: a(href="#sites") Sites
          span.line
        li.apps: a(href="#apps") Apps
          span.line
        li.code: a(href="#code") Code
          span.line
        li.contact: a(href="#contact") Contact
          span.line
      div.content.col4
        blocks = [1,2,3,4,5,6,7,8]
        each block in blocks
          div.box.col.site(style="background-image:url(ui/images/333.gif)")
            p Kindspark #{block}

    script(src="ui/vendor/jquery/jquery-1.7.2.min.js")
    script(src="ui/vendor/routie/routie.js")
    script(src="ui/scripts/app.js")
